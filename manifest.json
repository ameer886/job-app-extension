{
    "manifest_version": 3,
    "name": "Job Application AutoFiller",
    "description": "Automatically Fills in Job Application Form",
    "version": "1.0",
    "action": {
      "default_popup": "/popup/popup.html",
      "default_icon": "application.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }